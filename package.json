{
  "name": "babes",
  "version": "1.0.0",
  "description": "A Node micro-service for easily transpiling JavaScript with Babel.",
  "main": "app.js",
  "scripts": {
    "test": "babel-node test",
    "start": "micro ./server",
    "start-dev": "micro-dev ./server"
  },
